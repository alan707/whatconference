<%= simple_form_for conference do |f| %>
  <%= f.input :title, label: 'Title', placeholder: 'please add the conference title' %>
  <%= f.input :location, as: 'string', label: 'Location', placeholder: 'please add the location - e.g. Las Vegas, NV' %>
  <%= f.input :start_date, label: 'Start Date', as: :string, input_html: { class: 'dp', id: 'start_date' }  %>
  <%= f.input :end_date, label: 'End Date', as: :string, input_html: { class: 'dp', id: 'end_date' }  %>
  <%= f.input :url, as: 'url', label: 'Url', placeholder: 'copy conference url and paste here' %>
  <%= f.button :submit, "Submit", class: "btn btn-lg btn-primary" %>

<% end %>
