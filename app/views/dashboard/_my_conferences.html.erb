<div id="conference-browser">

  <div class="row">
    <div class="col-md-6">
  
      <div class="panel panel-default">
        <div class="panel-heading">Conferences</div>
        <table class="table table-striped table-hover" id="conference-list">
          <tr><td class="text-center"><i class="fa fa-2x fa-spinner fa-spin"></i></td></tr>
        </table>
      </div>
    </div>
  
    <div class="col-md-6">
      <div id="conference-browser-calendar"></div>
      <div class="map-container">
        <div class="map" id="conference-browser-map"></div>
      </div>
    </div>
  </div>
</div>
