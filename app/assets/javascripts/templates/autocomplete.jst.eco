<% unless @noMatch: %>
<p class="conference-title"><%= @title %></p>
<p class="conference-location"><%= @city_state %></p>
<p class="conference-date"><%= @date_range %></p>
<% else: %>
<p class="new-conference"><i class="fa fa-plus icon-left"></i> Add a new conference <span class="new-conference-title"><%= @query %></span></p>
<% end %>
